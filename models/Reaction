const { Schema, model, isObjectIdOrHexString } = require('mongoose');

const ReactionsSchema = new Schema({
    reactionId: {
        type: objectId,
        default: new objectId
    },
    reactionBody: {
        type: String,
        required: true,
        lenght: [280]
    },
    username: {
        type: String,
        required: true
    },
    createdAt:{
        type: Date,
        defult: Date.now,
        
    }
})